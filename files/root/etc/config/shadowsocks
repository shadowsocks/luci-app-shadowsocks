
config general
	option startup_delay '5'

config transparent_proxy
	list main_server 'nil'
	option udp_relay_server 'nil'
	option local_port '1234'

config socks5_proxy
	list server 'nil'
	option local_port '1080'

config port_forward
	list server 'nil'
	option local_port '5353'
	option destination '8.8.8.8'
	option destination_port '53'

config servers
	option alias 'sample'
	option fast_open '0'
	option no_delay '0'
	option server '127.0.0.1'
	option server_port '8388'
	option timeout '60'
	option password 'barfoo'
	option encrypt_method 'rc4-md5'

config access_control
	option wan_bp_list '/dev/null'
	list wan_bp_ips '0.0.0.0/1'
	list wan_bp_ips '128.0.0.0/1'
	option self_proxy '0'
	option lan_target 'RETURN'
